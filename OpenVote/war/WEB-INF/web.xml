<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<filter>
		<filter-name>ObjectifyFilter</filter-name>
		<filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ObjectifyFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<servlet>
		<servlet-name>OpenVote</servlet-name>
		<servlet-class>com.openvote.OpenVoteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OpenVote</servlet-name>
		<url-pattern>/openvote</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>results</servlet-name>
		<servlet-class>com.openvote.ResultsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>results</servlet-name>
		<url-pattern>/results</url-pattern>
	</servlet-mapping>	
	
	
	<servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.openvote.LoginServlet</servlet-class>
    </servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
        <servlet-name>castvote</servlet-name>
        <servlet-class>com.openvote.CastVoteServlet</servlet-class>
    </servlet>
	<servlet-mapping>
		<servlet-name>castvote</servlet-name>
		<url-pattern>/castvote</url-pattern>
	</servlet-mapping>
	

	<servlet>
        <servlet-name>castfakevote</servlet-name>
        <servlet-class>com.openvote.CastFakeVoteServlet</servlet-class>
    </servlet>
	<servlet-mapping>
		<servlet-name>castfakevote</servlet-name>
		<url-pattern>/castfakevote</url-pattern>
	</servlet-mapping>
	
	<servlet>
        <servlet-name>scrollvotes</servlet-name>
        <servlet-class>com.openvote.ScrollVotesServlet</servlet-class>
    </servlet>
	<servlet-mapping>
		<servlet-name>scrollvotes</servlet-name>
		<url-pattern>/scrollvotes</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
        <servlet-name>goodbye</servlet-name>
        <servlet-class>com.openvote.GoodbyeServlet</servlet-class>
    </servlet>
	<servlet-mapping>
		<servlet-name>goodbye</servlet-name>
		<url-pattern>/goodbye</url-pattern>
	</servlet-mapping>
	

	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>
